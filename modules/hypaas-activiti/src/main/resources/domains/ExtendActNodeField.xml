<?xml version="1.0" ?>
<domain-models xmlns="http://hypaas.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://hypaas.com/xml/ns/domain-models http://hypaas.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module name="activiti" package="com.hypaas.activiti.db"/>

  <entity name="ExtendActNodeField" cacheable="true" implements="java.io.Serializable">
    <string name="nodeId" title="Node Id"/><!--节点id-->
    <string name="fieldName" title="Field Name"/><!--字段名称-->
    <string name="fieldType" title="Field Type"/><!--字段类型  1=可写(可写的也能读) 2=参与连线判断-->
    <string name="rule" title="Rule"/><!--判断规则 "==",">=","<=",">","<"-->
    <string name="fieldVal" title="FieldVal"/><!--条件值-->
    <string name="elOperator" title="El Operator"/><!--el表达式 运算符 &&=并且，||=或-->
    <string name="sort" title="Sort"/><!--同一节点条件排序-->
  </entity>

</domain-models>
